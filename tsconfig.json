{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2022",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
